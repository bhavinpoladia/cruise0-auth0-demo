# Auth0 Configuration Template
# Copy this file to .env.local and replace with your actual values

# Your Auth0 tenant domain (found in Auth0 Dashboard → Applications → Settings)
REACT_APP_AUTH0_DOMAIN=your-tenant-name.us.auth0.com

# Your Auth0 application Client ID (found in Auth0 Dashboard → Applications → Settings)
REACT_APP_AUTH0_CLIENT_ID=your-client-id-here

# Redirect URI for your application
REACT_APP_AUTH0_REDIRECT_URI=http://localhost:3000